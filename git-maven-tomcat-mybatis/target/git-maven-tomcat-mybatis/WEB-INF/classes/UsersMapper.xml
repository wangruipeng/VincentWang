<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wrp.demo.dao.UsersMapper">
	<resultMap id="BaseResultMap" type="com.wrp.demo.model.Users">
		<result column="ID" property="id" jdbcType="VARCHAR" />
		<result column="NAME" property="name" jdbcType="VARCHAR" />
		<result column="AGE" property="age" jdbcType="VARCHAR" />
	</resultMap>
	<select id="findById" parameterType="String"
		resultType="com.wrp.demo.model.Users">
		select * from users where id=#{id}
	</select>
	<insert id="insert" parameterType="com.wrp.demo.model.Users">
		insert into USERS (ID, NAME, AGE
		)
		values (#{id,jdbcType=VARCHAR}, #{name,jdbcType=VARCHAR},
		#{age,jdbcType=VARCHAR}
		)
	</insert>
	<insert id="insertSelective" parameterType="com.wrp.demo.model.Users">
		insert into USERS
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="id != null">
				ID,
			</if>
			<if test="name != null">
				NAME,
			</if>
			<if test="age != null">
				AGE,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="id != null">
				#{id,jdbcType=VARCHAR},
			</if>
			<if test="name != null">
				#{name,jdbcType=VARCHAR},
			</if>
			<if test="age != null">
				#{age,jdbcType=VARCHAR},
			</if>
		</trim>
	</insert>
</mapper>